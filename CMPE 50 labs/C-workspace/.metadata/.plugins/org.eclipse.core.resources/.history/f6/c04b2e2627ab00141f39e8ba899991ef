#include <iostream>
#include <stdio.h>
using namespace std;


/*
 * Excercise one
 * 	function Overriding
 */
void Add(int type1, int type2);


int main(void)
{

	cout << "What would you like to print out?" << endl << "\t1. integer\n\t2. double\n\t3. Strings (character arrays)";
	return 0;
}


void  Add(int type1, int type2)
{
	cout << "You have entered two integers which are: " << type1 << " and" << type2;
}

void Add(double type1, double type2)
{
	cout << "You have entered two integers which are: " << type1 << " and" << type2;
}

void Add(float type1, float type2)
{
	cout << "You have entered two integers which are: " << type1 << " and" << type2;
}

