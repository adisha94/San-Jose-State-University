
public class Employee extends SeperateChainHashMap
{
	String name;
	private double salary;
	private int seniority;
	private int employee_hash;

	public Employee(String person)
	{
		this.name = person;
		seniority = 1;
		employee_hash = hashCode();
	}
	
	
	public boolean equals(Object rhs)
	{
		return this && name.equals((Employee)rhs.name);
	}
	
	/**
	 * Each object has its own hashValue
	 * for first time, compute normally, for everytime after, check to make sure hashcodes are not the same for the newly constructed objects in the hash table 
	 */
	public int hashCode1()
	{
		employee_hash = name.hashCode();
		return employee_hash;
	}
	
}