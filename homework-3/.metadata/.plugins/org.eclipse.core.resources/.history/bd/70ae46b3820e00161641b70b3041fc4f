import static org.junit.Assert.*;
import java.util.*;
import org.junit.Test;

public class EmployeeTest 
{

//	HashSet myMap = new HashSet<Employee>(); 
//	SeperateChainHashMap<Employee> map = new SeperateChainHashMap<>();
	@Test
	public void test() 
	{		
		ArrayList<Employee> myList = new ArrayList<>();
		myList.add(new Employee("Adish"));
		myList.add(new Employee("Hammad"));
		myList.add(new Employee("Tuan"));
		myList.add(new Employee("Kevin"));
		myList.add(new Employee("Bob"));
	
		PrintMap(myList);
		Collections.sort(myList);
		PrintMap(myList);
	}

	
	
	public void PrintMap(ArrayList<Employee> list)
	{
		for(Employee temp: list)
		{
			System.out.println("Employee name:  " + temp.getName());
		}
		
		System.out.println();
		System.out.println();
	}
}
